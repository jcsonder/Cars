        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cars"
        mc:Ignorable="d"
        Title="Cars" Height="350" Width="525">
    <Grid>
        <Button x:Name="button1" Content="Task" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" Click="button1_Click"/>
        <Button x:Name="button2" Content="Observable" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="497" Click="button2_Click"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="497"/>
    </Grid>
